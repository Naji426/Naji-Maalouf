//Problem02
   
let num1 =1;
let num2 =4;
let num3 =2;

if (num1>=num2+num3 && num1>=0) 
  console.log('true');
else if(num2 = num3**2 && num3 %2 ==1 )
  console.log('false');
else if(num1**8 > num1+num2+num3 || num1 + num2 == 0)
  console.log('undefined');
else 
  console.log('null');
